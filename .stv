stv() {
  if [[ $1 == "-v" ]]; then
    x=$@
    if [[ $2 == "ubuntu" ]]; then
        x=${x/-v ubuntu/}
        sudo apt $x
    elif [[ $2 == "arch" ]]; then
        x=${x/-v arch/}
        distrobox-enter --name arch  --command sudo pacman $x
    else
        echo "Command Not Found"
    fi
  fi
}
